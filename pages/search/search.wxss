page{
    background-color: #fff;
}
input{
    border-radius:  0 !important;
}
.header{
    background-color: #fff;
    border-bottom:1px solid #f3f3f3;
    padding-bottom: 14rpx;
    position: fixed;
    z-index: 77;
    width: 750rpx;
    top:0
}
.container{
    padding-top: 70rpx;
}
.history-title{
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-top: 38rpx;
}
.history-container{
    display: flex;
    flex-direction: column;
}
.slide{
    width: 8rpx;
    height: 46rpx;
    background-color: #157658;
    margin-right: 22rpx;
}
.history-title > text{
    font-size: 28rpx;
    color:#333;
}
.history-tags{
    width: 100%;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    margin-top: 20rpx;
    padding-left:20rpx;
    flex-wrap:wrap;
}
.tag{
    background-color: #f3f3f3 !important;
    color: #666 !important;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}
.tag-selected{
    background-color: #DCEBE6 !important;
    color:#157658 !important;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}
.spu-bottom{
    box-sizing: border-box;
    width: 100%;
    padding: 18rpx 18rpx 0 18rpx ;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    flex-wrap:wrap;
}